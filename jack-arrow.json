{
  "title": "karabienr rules http://jack-fan1991.github.io/KarabinerConfiguration/)",
  "rules": [
    {
      "description": "left_option + Arrow Keys, w/a/s/d/   ,/a/z/c/v/f/e/q/r ",
      "manipulators": [
        {
          "from": {
            "key_code": "left_option",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_option"
            }
          ],
          "to_if_held_down": [
